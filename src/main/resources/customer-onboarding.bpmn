      <bpmn:incoming>Flow_1w8nokc</bpmn:incoming>
      <bpmn:outgoing>Flow_105tv1b</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_105tv1b" sourceRef="validateCustomerData" targetRef="performCreditCheck" />
    <bpmn:serviceTask id="performCreditCheck" name="Perform Credit Check" camunda:delegateExpression="${creditCheckDelegate}">
      <bpmn:incoming>Flow_105tv1b</bpmn:incoming>
      <bpmn:outgoing>Flow_0gqppx2</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_0gqppx2" sourceRef="performCreditCheck" targetRef="assessRiskLevel" />
    <bpmn:serviceTask id="assessRiskLevel" name="Assess Risk Level" camunda:delegateExpression="${assessRiskDelegate}">
      <bpmn:incoming>Flow_0gqppx2</bpmn:incoming>
      <bpmn:outgoing>Flow_04r16jh</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="gatewayHighRisk" name="High Risk?">
      <bpmn:incoming>Flow_04r16jh</bpmn:incoming>
      <bpmn:outgoing>Flow_0xbg5qc</bpmn:outgoing>
      <bpmn:outgoing>Flow_0x8ht8p</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_04r16jh" sourceRef="assessRiskLevel" targetRef="gatewayHighRisk" />
    <bpmn:sequenceFlow id="Flow_0xbg5qc" name="High Risk" sourceRef="gatewayHighRisk" targetRef="manualApproval">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${riskLevel == 'HIGH'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="manualApproval" name="Manual Approval Required" camunda:assignee="demo">
      <bpmn:extensionElements>
        <camunda:formData>
          <camunda:formField id="approved" label="Approve Customer?" type="boolean" defaultValue="false" />
          <camunda:formField id="approvalComments" label="Comments" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0xbg5qc</bpmn:incoming>
      <bpmn:outgoing>Flow_0682ofr</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_1r0e0az" name="Approved?">
      <bpmn:incoming>Flow_0682ofr</bpmn:incoming>
      <bpmn:outgoing>Flow_0ayd9j0</bpmn:outgoing>
      <bpmn:outgoing>Flow_1j70moq</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sequenceFlow id="Flow_0682ofr" sourceRef="manualApproval" targetRef="Gateway_1r0e0az" />
    <bpmn:sequenceFlow id="Flow_0ayd9j0" name="Approved" sourceRef="Gateway_1r0e0az" targetRef="createAccount">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_1j70moq" name="Rejected" sourceRef="Gateway_1r0e0az" targetRef="sendRejectionEmail">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0x8ht8p" name="Low/Medium Risk" sourceRef="gatewayHighRisk" targetRef="createAccount">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${riskLevel != 'HIGH'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:endEvent id="Event_0q69iin" name="Customer Rejected">
      <bpmn:incoming>Flow_0ocslh3</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0ocslh3" sourceRef="sendRejectionEmail" targetRef="Event_0q69iin" />
    <bpmn:serviceTask id="createAccount" name="Create Account" camunda:delegateExpression="${accountCreationDelegate}">
      <bpmn:incoming>Flow_0ayd9j0</bpmn:incoming>
      <bpmn:incoming>Flow_0x8ht8p</bpmn:incoming>
      <bpmn:outgoing>Flow_10yk3jb</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="sendRejectionEmail" name="Send Rejection Email" camunda:delegateExpression="${rejectionEmailDelegate}">
      <bpmn:incoming>Flow_1j70moq</bpmn:incoming>
      <bpmn:outgoing>Flow_0ocslh3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="Flow_10yk3jb" sourceRef="createAccount" targetRef="sendWelcomeEmail" />
    <bpmn:serviceTask id="sendWelcomeEmail" name="Send Welcome Email" camunda:delegateExpression="${welcomeEmailDelegate}">
      <bpmn:incoming>Flow_10yk3jb</bpmn:incoming>
      <bpmn:outgoing>Flow_04eixg1</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_1cdnh4m" name="Customer Enboarded">
      <bpmn:incoming>Flow_04eixg1</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_04eixg1" sourceRef="sendWelcomeEmail" targetRef="Event_1cdnh4m" />
  </bpmn:process>
